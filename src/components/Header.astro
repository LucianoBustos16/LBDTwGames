---
export interface Props {
	title: string;
}

const { title } = Astro.props;
---

<header class="p-6 max-w-sm mx-auto rounded-xl flex justify-center space-x-4">
    <nav>
        <img class="w-16" src="https://i0.wp.com/lbdtw.com.ar/wp-content/uploads/cropped-ico.png?w=512&ssl=1" alt={title}>
    </nav>
</header>